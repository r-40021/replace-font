{
  "manifest_version": 3,
  "name": "Replace Font",
  "version": "1.1.0", 
  "description": "見ていて不快になるフォントを置き換える拡張機能",
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "css": ["style.css"]
    }
  ],
  "web_accessible_resources": [
    {
      "resources": [
        "font/*.otf"
      ],
      "matches": [
        "<all_urls>"
      ]
    }
  ]
}